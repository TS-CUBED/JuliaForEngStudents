@article{Cheng2005,
  title = {Fluid-Structure Coupled {{CFD}} Simulation of the Left Ventricular Flow during Filling Phase.},
  author = {Cheng, Yongguang and Oertel, Herbert and Schenkel, Torsten},
  year = {2005},
  month = may,
  journal = {Annals of Biomedical Engineering},
  volume = {33},
  number = {5},
  pages = {567--576},
  issn = {0090-6964},
