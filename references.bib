@article{VanDoormaal2012,
  title = {Haemodynamics in the Mouse Aortic Arch Computed from {{MRI-derived}} Velocities at the Aortic Root.},
  author = {a Van Doormaal, Mark and Kazakidi, Asimina and Wylezinska, Marzena and Hunt, Anthony and Tremoleda, Jordi L and Protti, Andrea and Bohraus, Yvette and Gsell, Willy and Weinberg, Peter D and Ethier, C Ross},
  year = {2012},
