}

@article{Demirbas2010,
  title = {Methane Hydrates as Potential Energy Resource: Part 1-Importance, Resource and Recovery Facilities},
  author = {Demirbas, Ayhan},
  year = {2010},
  month = jul,
  journal = {ENERGY CONVERSION AND MANAGEMENT},
