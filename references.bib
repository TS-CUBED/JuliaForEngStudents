@article{Savabi2020,
  title = {Fluid Structure Interaction Modeling of Aortic Arch and Carotid Bifurcation as the Location of Baroreceptors},
  author = {Savabi, Reza and Nabaei, Malikeh and Farajollahi, Sami and Fatouraee, Nasser},
  year = {2020},
