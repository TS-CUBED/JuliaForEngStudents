@article{Wu2015,
  title = {Fluid\textendash{{Structure Interaction Model}} of a {{Percutaneous Aortic Valve}}: {{Comparison}} with an {{In Vitro Test}} and {{Feasibility Study}} in a {{Patient-Specific Case}}},
  shorttitle = {Fluid\textendash{{Structure Interaction Model}} of a {{Percutaneous Aortic Valve}}},
  author = {Wu, Wei and Pott, Desiree and Mazza, Beniamino and Sironi, Tommaso and Dordoni, Elena and Chiastra, Claudio and Petrini, Lorenza and Pennati, Giancarlo and Dubini, Gabriele and Steinseifer, Ulrich and Sonntag, Simon and Kuetting, Maximilian and Migliavacca, Francesco},
