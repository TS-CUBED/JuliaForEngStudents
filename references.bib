@article{Jamshidi2019,
  title = {On Suitability of Phase-Field and Algebraic Volume-of-Fluid {{OpenFOAM}}\textregistered{} Solvers for Gas\textendash Liquid Microfluidic Applications},
  author = {Jamshidi, F. and Heimel, H. and Hasert, M. and Cai, X. and Deutschmann, O. and Marschall, H. and W{\"o}rner, M.},
