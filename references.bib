  author = {Mazzi, Valentina and Morbiducci, Umberto and Cal{\`o}, Karol and De Nisco, Giuseppe and Lodi Rizzini, Maurizio and Torta, Elena and Caridi, Giuseppe Carlo Alp and Chiastra, Claudio and Gallo, Diego},
