@article{Kabinejadian2014,
  title = {In Vitro Measurements of Velocity and Wall Shear Stress in a Novel Sequential Anastomotic Graft Design Model under Pulsatile Flow Conditions},
  author = {Kabinejadian, Foad and Ghista, Dhanjoo N. and Su, Boyang and Kaabi Nezhadian, Mercedeh and Chua, Leok Poh and Yeo, Joon Hock and Leo, Hwa Liang},
