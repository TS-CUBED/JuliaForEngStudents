@article{Biasetti2014,
  title = {Synergy between Shear-Induced Migration and Secondary Flows on Red Blood Cells Transport in Arteries: Considerations on Oxygen Transport},
  author = {Biasetti, Jacopo and Spazzini, Pier Giorgio and Hedin, Ulf and Gasser, T. Christian},
  year = {2014},
  journal = {Journal of the Royal Society, Interface},
