  keywords = {Animals,Aorta,Aorta: physiology,Blood Flow Velocity,Blood Flow Velocity: physiology,Blood Pressure,Blood Pressure: physiology,Cardiovascular,Computer Simulation,Elasticity,Finite Element Analysis,Inbred C57BL,Inbred Strains,Male,Mechanical,Mice,Models,Stress},
