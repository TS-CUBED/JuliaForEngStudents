@article{Sarrami-Foroushani2015,
  title = {Modeling of the Acute Effects of Primary Hypertension and Hypotension on the Hemodynamics of Intracranial Aneurysms.},
  author = {{Sarrami-Foroushani}, Ali and {Villa-Uriol}, Maria-Cruz and Nasr Esfahany, Mohsen and Coley, Stuart C and Di Marco, Luigi Yuri and Frangi, Alejandro F and Marzo, Alberto},
  year = {2015},
  month = jan,
  journal = {Annals of biomedical engineering},
  volume = {43},
  number = {1},
  eprint = {25118666},
  eprinttype = {pubmed},
  pages = {207--21},
  issn = {1573-9686},
  doi = {10.1007/s10439-014-1076-7},
  abstract = {Hemodynamics is a risk factor in intracranial aneurysms (IA). Hypertension and pharmacologically induced hypotension are common in IA patients. This study investigates how hypertension and hypotension may influence aneurysmal hemodynamics. Images of 23 IAs at typical locations were used to build patient-specific Computational Fluid Dynamics models. The effects of hypotension and hypertension were simulated through boundary conditions by modulating the normotensive flow and pressure waveforms, in turn produced by a 1D systemic vascular model. Aneurysm location and flow pattern types were used to categorize the influence of hypotension and hypertension on relevant flow variables (velocity, pressure and wall shear stress). Results indicate that, compared to other locations, vertebrobasilar aneurysms (VBA) are more sensitive to flow changes. In VBAs, space-averaged velocity at peak systole increased by 30\% in hypertension (16-21\% in other locations). Flow in VBAs in hypotension decreased by 20\% (10-13\% in other locations). Momentum-driven hemodynamic types were also more affected by hypotension and hypertension, than shear-driven types. This study shows how patient-specific modeling can be effectively used to identify location-specific flow patterns in a clinically-relevant study, thus reinforcing the role played by modeling technologies in furthering our understanding of cardiovascular disease, and their potential in future healthcare.},
  pmid = {25118666},
  keywords = {1d circulation model,Blood Pressure,boundary conditions,cerebral vasculature,computational,human,simulation,virtual physiological,wall shear stress},
  file = {/home/thor/Zotero/storage/S7FJ6ZFS/Sarrami-Foroushani et al. - 2015 - Modeling of the acute effects of primary hypertens.pdf}
}

@article{Sarratt2004,
  title = {Platelet {{Receptor Glycoprotein VI-Mediated Adhesion}} to {{Type I Collagen}} under {{Hydrodynamic Flow}}},
  author = {Sarratt, Kendra L and Chen, Hong and Kahn, Mark L and Hammer, Daniel A},
  year = {2004},
  journal = {Biomedical Engineering Society},
  abstract = {Glycoprotein VI (GPVI) is a platelet receptor that directly binds collagen. [1 has been shown by expressing GPVI in rat basophilic leukemia (RBL-2H3) cells that GPVI mediates adhesion to type I collagen under static conditions. However, the ability of GPVI to secure adhesion to collagen type I under ftow has not been measured. We studied the interaction of GPVI and type I collagen under hydrodynamic ftow using RBL-2H3 cells transfected with the GPVI receptor. We found that GPVI-expressing RBL-2H3 cells adhere to collagen under ftow, significantly more so than non-GPVI-expressing RBL-2H3 cells. Inhibition of GPVI by the I I A 12 anti-GPVI antibody significantIy blocks adhesion to collagen, indicating that GPVI specifically interacts with collagen. Probing the role of signaling in GPVI binding to collagen, we used mutants ofGPVI and observed that signal transduction did not illhibit adhesion. To test the correlation between receptor expression and adhesion, we tested three GPVI-expressing RBL-2H3 cell lines (A. B. and C) with different levels of receptor expression. At a single shear rate, the level of adhesion increases monotonically with surrace expression. The results, using this model cell linc, indicate that GPVI is capable of mediating adhesion to collagen under shear, in a density-dependent fashion that is independent of GPVI signaling. Keywords-Rat basophilic leukemia c(!lls, Shear rate, Signaling, Mutant.}
}

