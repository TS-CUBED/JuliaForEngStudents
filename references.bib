  author = {{Von Knobelsdorff-Brenkenhoff}, Florian and Karunaharamoorthy, Achudhan and Trauzeddel, Ralf Felix and Barker, Alex J. and Blaszczyk, Edyta and Markl, Michael and {Schulz-Menger}, Jeanette},
