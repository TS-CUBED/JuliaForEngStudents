@article{Blume2006,
  title = {Outcomes of Children Bridged to Heart Transplantation with Ventricular Assist Devices: A Multi-Institutional Study},
  author = {Blume, Elizabeth D and Naftel, David C and Bastardi, Heather J and Duncan, Brian W and Kirklin, James K and Webber, Steven A and {for the Pediatric Heart Transplant Study Investigators}},
  year = {2006},
  journal = {Circulation},
  volume = {113},
