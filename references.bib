@article{Eriksson2013,
  title = {Four-Dimensional Blood Flow-Specific Markers of {{LV}} Dysfunction in Dilated Cardiomyopathy},
  author = {Eriksson, Jonatan and Bolger, Ann F. and Ebbers, Tino and Carlh{\"a}ll, Carl Johan},
  year = {2013},
  journal = {European Heart Journal Cardiovascular Imaging},
  volume = {14},
  number = {5},
  issn = {2047-2404},
  doi = {10.1093/ehjci/jes159},
  abstract = {AIMS: Patients with mild heart failure (HF) who are clinically compensated may have normal left ventricular (LV) stroke volume (SV). Despite this, altered intra-ventricular flow patterns have been recognized in these subjects. We hypothesized that, compared with normal LVs, flow in myopathic LVs would demonstrate a smaller proportion of inflow volume passing directly to ejection and diminished the end-diastolic preservation of the inflow kinetic energy (KE).\textbackslash n\textbackslash nMETHODS AND RESULTS: In 10 patients with dilated cardiomyopathy (DCM) (49 {$\pm$} 14 years, six females) and 10 healthy subjects (44 {$\pm$} 17 years, four females), four-dimensional MRI velocity and morphological data were acquired. A previously validated method was used to separate the LV end-diastolic volume (EDV) into four flow components based on the blood's locations at the beginning and end of the cardiac cycle. KE was calculated over the cardiac cycle for each component. The EDV was larger (P = 0.021) and the ejection fraction smaller (P {$<$} 0.001) in DCM compared with healthy subjects; the SV was equivalent (DCM: 77 {$\pm$} 19, healthy: 79 {$\pm$} 16 mL). The proportion of the total LV inflow that passed directly to ejection was smaller in DCM (P = 0.000), but the end-diastolic KE/mL of the direct flow was not different in the two groups (NS).\textbackslash n\textbackslash nCONCLUSION: Despite equivalent LVSVs, HF patients with mild LV remodelling demonstrate altered diastolic flow routes through the LV and impaired preservation of inflow KE at pre-systole compared with healthy subjects. These unique flow-specific changes in the flow route and energetics are detectable despite clinical compensation, and may prove useful as subclinical markers of LV dysfunction.},
  file = {/home/thor/Zotero/storage/8LU5L2IE/Eriksson et al. - 2013 - Four-dimensional blood flow-specific markers of LV.pdf}
}

@inproceedings{Ersland2009,
  title = {Transport and Storage of {{CO}}(2) in Natural Gas Hydrate Reservoirs},
  booktitle = {{{GREENHOUSE GAS CONTROL TECHNOLOGIES}} 9},
  author = {Ersland, Geir and Husebo, Jarle and Graue, Arne and Kvamme, Bjorn},
  editor = {Gale, J and Herzog, H and Braitsch, J},
