@article{Nunez2018,
  title = {Optimizing Colormaps with Consideration for Color Vision Deficiency to Enable Accurate Interpretation of Scientific Data},
  author = {Nu{\~n}ez, Jamie R. and Anderton, Christopher R. and Renslow, Ryan S.},
  year = {2018},
