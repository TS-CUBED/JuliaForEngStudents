  title = {Contribution of the Diastolic Vortex Ring to Left Ventricular Filling},
  author = {{Mart{\'i}nez-Legazpi}, Pablo and Bermejo, Javier and Benito, Yolanda and Yotti, Raquel and P{\'e}rez Del Villar, Candelas and {Gonz{\'a}lez-Mansilla}, Ana and Barrio, Alicia and Villacorta, Eduardo and S{\'a}nchez, Pedro L. and {Fern{\'a}ndez-Avil{\'e}s}, Francisco and Del {\'A}lamo, Juan C.},
